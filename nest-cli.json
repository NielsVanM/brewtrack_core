{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/api_gate/src",
  "monorepo": true,
  "root": "apps/api_gate",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/api_gate/tsconfig.app.json"
  },
  "projects": {
    "api_gate": {
      "type": "application",
      "root": "apps/api_gate",
      "entryFile": "main",
      "sourceRoot": "apps/api_gate/src",
      "compilerOptions": {
        "tsConfigPath": "apps/api_gate/tsconfig.app.json"
      }
    },
    "auth_service": {
      "type": "application",
      "root": "apps/auth_service",
      "entryFile": "main",
      "sourceRoot": "apps/auth_service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth_service/tsconfig.app.json"
      }
    }
  }
}